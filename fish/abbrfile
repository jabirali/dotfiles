# Version control.
g:  git
ga: git add
gb: git branch
gc: git commit
gd: git diff
gf: git fetch
gh: git help
gm: git merge
gp: git push
gs: git status
gr: git reset

# Package management.
a:  sudo apt
ai: sudo apt install
ar: sudo apt autoremove
as: apt search
ah: apt help

s:  sudo snap
si: sudo snap install
sr: sudp snap remove
sh: snap help
ss: snap search

